@model Akkoc.Domain.POCO.Static

    <footer>
        <div class="container">
            <div class="row">
                <div class="span6">
                    <div class="widget">
                        <h5 class="widgetheading">Browse pages</h5>
                        <ul class="link-list">
                            <li><a href="#">About our company</a></li>
                            <li><a href="#">Our services</a></li>
                            <li><a href="#">Meet our team</a></li>
                            <li><a href="#">Explore our portfolio</a></li>
                            <li><a href="#">Get in touch with us</a></li>
                        </ul>
                    </div>

                </div>
                <div class="span6">
                    <div class="widget">
                        <h5 class="widgetheading">Get in touch with us</h5>
                        <address>
                            <strong>@Model.Address</strong><br>
                            @Model.Instagram<br>
                        </address>
                        <p>
                            <i class="icon-phone"></i> @Model.Tel1<br>
                            <i class="icon-envelope-alt"></i> @Model.Email1
                        </p>
                    </div>
                </div>
            </div>
        </div>
        <div id="sub-footer">
            <div class="container">
                <div class="row">
                    <div class="span6">
                        <div class="copyright">
                            <p>
                                <span>@Model.Copyrigth</span>
                            </p>
                            <div class="credits">
                                <!--
                                  All the links in the footer should remain intact.
                                  You can delete the links only if you purchased the pro version.
                                  Licensing information: https://bootstrapmade.com/license/
                                  Purchase the pro version with working PHP/AJAX contact form: https://bootstrapmade.com/buy/?theme=Flattern
                                -->
                                Designed by <a href="https://bootstrapmade.com/">BootstrapMade</a>
                            </div>
                        </div>
                    </div>
                    <div class="span6">
                        <ul class="social-network">
                            <li><a href="@Model.FacebookUrl" data-placement="bottom" title="Facebook"><i class="icon-facebook icon-square"></i></a></li>
                            <li><a href="@Model.PinterestUrl" data-placement="bottom" title="Pinterest"><i class="icon-pinterest icon-square"></i></a></li>
                            <li><a href="@Model.InstagramUrl" data-placement="bottom" title="Instagram"><i class="icon-linkedin icon-square"></i></a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </footer>
