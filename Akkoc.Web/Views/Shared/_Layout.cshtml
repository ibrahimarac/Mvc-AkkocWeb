
<!DOCTYPE html>
<html lang="tr">

<head>
    <meta charset="utf-8">
    <title>@ViewBag.Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content="" />
    <meta name="author" content="" />

    <!-- css -->
    <link href="https://fonts.googleapis.com/css?family=Noto+Serif:400,400italic,700|Open+Sans:300,400,600,700" rel="stylesheet">
    <link href="~/Content/template/css/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/template/css/bootstrap-responsive.css" rel="stylesheet" />
    <link href="~/Content/template/css/fancybox/jquery.fancybox.css" rel="stylesheet">
    <link href="~/Content/template/css/jcarousel.css" rel="stylesheet" />
    <link href="~/Content/template/css/flexslider.css" rel="stylesheet" />
    <link href="~/Content/template/css/cslider.css" rel="stylesheet" />
    <link href="~/Content/template/css/style.css" rel="stylesheet" />
    <!-- Theme skin -->
    <link id="t-colors" href="~/Content/template/skins/default.css" rel="stylesheet" />
    <!-- boxed bg -->
    <link id="bodybg" href="~/Content/template/bodybg/bg1.css" rel="stylesheet" type="text/css" />
    <!-- Fav and touch icons -->
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="~/Content/template/ico/apple-touch-icon-144-precomposed.png" />
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="~/Content/template/ico/apple-touch-icon-114-precomposed.png" />
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="~/Content/template/ico/apple-touch-icon-72-precomposed.png" />
    <link rel="apple-touch-icon-precomposed" href="~/Content/template/ico/apple-touch-icon-57-precomposed.png" />
    <link rel="shortcut icon" href="~/Content/template/ico/favicon.png" />

    @RenderSection("Styles",false)

    <!-- =======================================================
      Theme Name: Flattern
      Theme URL: https://bootstrapmade.com/flattern-multipurpose-bootstrap-template/
      Author: BootstrapMade.com
      Author URL: https://bootstrapmade.com
    ======================================================= -->
</head>

<body>
    <div id="wrapper">

        <!-- start header -->
        <header>
            <div class="container">
                @Html.Partial("_TopMenu")
            </div>
        </header>
        <!-- end header -->

        <section id="inner-headline">
            <div class="container">
                <div class="row">
                    <div class="span4">
                        <div class="inner-heading">
                            <h2>@ViewBag.Title</h2>
                        </div>
                    </div>
                    <div class="span8">
                        <ul class="breadcrumb">
                            <li><a href="/Home/Index"><i class="icon-home"></i></a><i class="icon-angle-right"></i></li>
                            <li><a href="@((Request.UrlReferrer!=null)?Request.UrlReferrer.AbsolutePath:"#") ">Önceki Ekran</a><i class="icon-angle-right"></i></li>
                            <li class="active">@ViewBag.Title</li>
                        </ul>
                    </div>
                </div>
            </div>
        </section>


        @RenderBody()


        @{Html.RenderAction("Static", "Home");}
    </div>
    <a href="#" class="scrollup"><i class="icon-chevron-up icon-square icon-32 active"></i></a>
    <!-- javascript
      ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="~/Content/template/js/jquery.js"></script>
    <script src="~/Content/template/js/jquery.easing.1.3.js"></script>
    <script src="~/Content/template/js/bootstrap.js"></script>
    <script src="~/Content/template/js/jcarousel/jquery.jcarousel.min.js"></script>
    <script src="~/Content/template/js/jquery.fancybox.pack.js"></script>
    <script src="~/Content/template/js/jquery.fancybox-media.js"></script>
    <script src="~/Content/template/js/google-code-prettify/prettify.js"></script>
    <script src="~/Content/template/js/portfolio/jquery.quicksand.js"></script>
    <script src="~/Content/template/js/portfolio/setting.js"></script>
    <script src="~/Content/template/js/jquery.flexslider.js"></script>
    <script src="~/Content/template/js/jquery.nivo.slider.js"></script>
    <script src="~/Content/template/js/modernizr.custom.js"></script>
    <script src="~/Content/template/js/jquery.ba-cond.min.js"></script>
    <script src="~/Content/template/js/jquery.slitslider.js"></script>
    <script src="~/Content/template/js/modernizr.custom.28468.js"></script>
    <script src="~/Content/template/js/jquery.cslider.js"></script>
    <script src="~/Content/template/js/animate.js"></script>

    <!-- Template Custom JavaScript File -->
    <script src="~/Content/template/js/custom.js"></script>
    <script src="~/Content/js/site-script.js"></script>

    <script src="https://www.google.com/recaptcha/api.js" async defer></script>

    @RenderSection("Scripts", false)

    <script>
        $.MenuClicked();
    </script>

</body>

</html>


