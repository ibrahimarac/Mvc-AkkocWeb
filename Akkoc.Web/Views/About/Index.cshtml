
@model Static

<section id="content">
    <div class="container">
        <div class="row">
            <div class="span6">
                <h2>@Model.AboutTitle</h2>
                <p>
                    @Html.Raw(Model.AboutDescription)
                </p>
            </div>
            <div class="span6">
                <!-- start flexslider -->
                <video width="320" height="240" controls>
                    <source src="~/Content/video/@Model.VideoFile" type="video/mp4">
                    Tarayıcınız html5 video oynatıcısını desteklemiyor.
                </video>
                <!-- end flexslider -->
            </div>
        </div>
        <!-- divider -->
        <div class="row">
            <div class="span12">
                <div class="solidline">
                </div>
            </div>
        </div>

    </div>
</section>

