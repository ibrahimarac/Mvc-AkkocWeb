    <link rel="apple-touch-icon-precomposed" href="~/Content/template/ico/apple-touch-icon-57-precomposed.png" />
    <link rel="shortcut icon" href="~/Content/template/ico/favicon.png" />

    <!-- =======================================================
      Theme Name: Flattern
