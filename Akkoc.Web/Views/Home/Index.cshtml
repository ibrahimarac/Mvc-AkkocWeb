@{ 
    Layout = null;
}


<!DOCTYPE html>
<html lang="tr">

<head>
    <meta charset="utf-8">
    <title>@ViewBag.Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content="" />
    <meta name="author" content="" />

    <!-- css -->
    <link href="https://fonts.googleapis.com/css?family=Noto+Serif:400,400italic,700|Open+Sans:300,400,600,700" rel="stylesheet">
    <link href="~/Content/template/css/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/template/css/bootstrap-responsive.css" rel="stylesheet" />
    <link href="~/Content/template/css/fancybox/jquery.fancybox.css" rel="stylesheet">
    <link href="~/Content/template/css/jcarousel.css" rel="stylesheet" />
    <link href="~/Content/template/css/flexslider.css" rel="stylesheet" />
    <link href="~/Content/template/css/cslider.css" rel="stylesheet" />
    <link href="~/Content/template/css/style.css" rel="stylesheet" />
    <!-- Theme skin -->
    <link id="t-colors" href="~/Content/template/skins/default.css" rel="stylesheet" />
    <!-- boxed bg -->
    <link id="bodybg" href="~/Content/template/bodybg/bg1.css" rel="stylesheet" type="text/css" />
    <!-- Fav and touch icons -->
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="~/Content/template/ico/apple-touch-icon-144-precomposed.png" />
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="~/Content/template/ico/apple-touch-icon-114-precomposed.png" />
